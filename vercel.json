{
  "version": 2,
  "builds": [
    {
      "src": "frontend/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist",
        "cleanDistDir": true
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "https://v-b3.onrender.com/api/$1",
      "headers": {
        "Content-Type": "application/json"
      }
    },
    {
      "src": "^/assets/(.*)",
      "headers": {
        "Cache-Control": "public, max-age=31536000, immutable"
      }
    },
    {
      "src": "^/assets/(.*)\\.(js|css|woff|woff2|ttf|eot|svg|png|jpg|jpeg|gif|ico|webp|avif)$",
      "dest": "/assets/$1.$2",
      "headers": {
        "Cache-Control": "public, max-age=31536000, immutable"
      }
    },
    {
      "src": "^/assets/(.*)",
      "dest": "/assets/$1"
    },
    {
      "src": "^/(.*\\.(js|css|json|ico|png|jpg|jpeg|svg|gif))$",
      "dest": "/$1"
    },
    {
      "src": "/[^.]*$",
      "dest": "/index.html"
    }
  ]
}
