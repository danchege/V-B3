#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/USER/Desktop/V-B3/frontend/node_modules/.pnpm/vite@7.0.6_jiti@1.21.7_yaml@2.8.1/node_modules/vite/bin/node_modules:/mnt/c/Users/USER/Desktop/V-B3/frontend/node_modules/.pnpm/vite@7.0.6_jiti@1.21.7_yaml@2.8.1/node_modules/vite/node_modules:/mnt/c/Users/USER/Desktop/V-B3/frontend/node_modules/.pnpm/vite@7.0.6_jiti@1.21.7_yaml@2.8.1/node_modules:/mnt/c/Users/USER/Desktop/V-B3/frontend/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/USER/Desktop/V-B3/frontend/node_modules/.pnpm/vite@7.0.6_jiti@1.21.7_yaml@2.8.1/node_modules/vite/bin/node_modules:/mnt/c/Users/USER/Desktop/V-B3/frontend/node_modules/.pnpm/vite@7.0.6_jiti@1.21.7_yaml@2.8.1/node_modules/vite/node_modules:/mnt/c/Users/USER/Desktop/V-B3/frontend/node_modules/.pnpm/vite@7.0.6_jiti@1.21.7_yaml@2.8.1/node_modules:/mnt/c/Users/USER/Desktop/V-B3/frontend/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../vite/bin/vite.js" "$@"
else
  exec node  "$basedir/../vite/bin/vite.js" "$@"
fi
